{
  "name": "mrm-tasks",
  "private": true,
  "repository": "git@github.com:chooin/mrm-tasks.git",
  "author": "Devin.Zhu <chooin@126.com>",
  "license": "MIT",
  "scripts": {
    "preinstall": "node ./scripts/check-yarn.js",
    "build": "npm-run-all build:*",
    "build:next": "yarn workspace mrm-task-next build",
    "build:react-native": "yarn workspace mrm-task-react-native build",
    "np:next": "cd packages/mrm-task-next && np && cd -",
    "np:react-native": "cd packages/mrm-task-react-native && np && cd -"
  },
  "workspaces": [
    "packages/*"
  ],
  "devDependencies": {
    "@types/node": "^15.0.2",
    "np": "^7.5.0",
    "npm-run-all": "^4.1.5",
    "typescript": "^4.2.4"
  }
}
